{
  "name": "openvino-lpr",
  "version": "1.0.0",
  "type": "pipeline",
  "device": "GPU",
  "precision": "FP16",

  "detector": {
    "format": "openvino-ir",
    "labels": "detector/labels.txt",
    "input_size": [672, 384],
    "path_xml": "detector/vehicle-license-plate-detection-barrier-0106.xml",
    "path_bin":  "detector/vehicle-license-plate-detection-barrier-0106.bin",
    "class_for_roi": "license-plate",
    "confidence_threshold": 0.45,
    "nms_threshold": 0.45
  },

  "recognizer": {
    "format": "openvino-ir",
    "path_xml": "recognizer/license-plate-recognition-barrier-0007.xml",
    "path_bin":  "recognizer/license-plate-recognition-barrier-0007.bin",
    "alphabet": "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ- ",
    "max_seq_len": 10
  },

  "runtime": {
    "num_streams": 1,
    "performance_hint": "LATENCY"
  },

  "outputs": {
    "text_attribute": "plate",
    "attach_to": "license-plate"
  }
}
